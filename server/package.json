{
  "name": "joke-factory",
  "version": "1.0.0",
  "description": "joke-factory app. A portfolio project for practicing TDD, TypeScript, and Express",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start:server": "NODE_ENV=development node dist/index.js",
    "watch": "NODE_ENV=development nodemon dist/index.js",
    "compile:watch": "tsc --watch -p tsconfig.build.json",
    "compile": "tsc -p tsconfig.build.json",
    "prestart": "npm run clean && npm run compile ",
    "start": "npm run compile:watch & npm run watch",
    "deploy": "npm run clean && npm compile && npm start:server",
    "test": "NODE_ENV=test jest",
    "test:inband": "NODE_ENV=test jest --runInBand",
    "test:inband:coverage": "NODE_ENV=test jest --coverage --runInBand",
    "test:inband:watch": "NODE_ENV=test jest --watchAll --runInBand",
    "test:one": "jest --testPathPattern=TokenService.test.ts",
    "test:one:watch": "jest --testPathPattern=Password.test.ts --watchAll",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "test:watch:inband": "NODE_ENV=test jest --runInBand --watchAll",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "lint": "eslint .",
    "ts:check": "tsc -p .",
    "check": "npm run lint && npm run ts:check"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/connect-flash": "^0.0.37",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.15.11",
    "@types/nodemailer": "^6.4.7",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/smtp-server": "^3.5.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "jest-watch-typeahead": "^2.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "smtp-server": "^3.11.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "connect-flash": "^0.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "i18next": "^22.4.14",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-middleware": "^3.3.0",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.9.1",
    "npm": "^9.6.4",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "sequelize": "^6.31.0",
    "sqlite3": "^5.1.6"
  }
}